				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "ppl_classwork_csv",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
